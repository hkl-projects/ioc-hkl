# FROM EPICS TO PYTHON

record(ao, "hklApp:hklWavelength") {
    field(PINI, "YES")
    field(DTYP, "pydev")
    field(OUT, "@hklCalculator.hkl_wavelength='VAL'")
    info(autosaveFields, "VAL")
}

